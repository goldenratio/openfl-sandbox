<?xml version="1.0" encoding="utf-8"?>
<project>
	
	<meta title="FontTest" package="com.example.fonttest" version="1.0.0" company="Company Name" />
	<app main="Main" path="Export" file="FontTest" />

    <window background="#000000" fps="60" hardware="true" vsync="true" />
    
    <window width="0" height="0" if="js" />
    <window width="600" height="600" if="flash" />
    <window width="600" height="600" if="desktop" />
    <window orientation="landscape" width="0" height="0" vsync="false" antialiasing="0" if="mobile" />

	<source path="Source" />
	
	<haxelib name="openfl" />
	
	<assets path="Assets" rename="assets" exclude="openfl.svg" />
	<icon path="Assets/openfl.svg" />

    <!-- fonts for html5 works only using DOM -->
    <haxedef name="dom" if="web"/>

</project>
